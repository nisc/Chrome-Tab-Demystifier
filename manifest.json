{
  "name": "Tab Demystifier",
  "version": "0.2.1",
  "description": "Shows the external referrer of newly created tabs. Useful if you can no longer remember why you've opened a certain tab.",
  "browser_action": {
    "default_icon": "icons/icon_off_19.jpg",
    "default_title": "No external referrer",
    "default_popup": ""
  },
  "background_page": "background.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "history"
  ],
  "icons": {
    "16": "icons/icon_16.jpg",
    "48": "icons/icon_48.jpg",
    "128": "icons/icon_128.jpg"
  }
  //content_security_policy": "default-src 'self' 'unsafe-inline' "
}
